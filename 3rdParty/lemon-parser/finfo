<script>
function updateClock(){
  var e = document.getElementById("clock");
  if(e){
    var d = new Date();
    e.innerHTML=d.toISOString().replace("T"," ").replace(/:\d\d\.\d+Z/,"");
    setTimeout("updateClock();",(60-d.getSeconds())*1000);
  }
}
updateClock();
</script>
